{
  "name": "revelation-resolver",
  "version": "0.1.4",
  "description": "Fast node.js modules resolver",
  "keywords": [
    "resolver",
    "module",
    "path",
    "fs"
  ],
  "files": ["dist", "!dist/**/*.snap", "!dist/**/*.spec.*"],
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "build": "tsc",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts --cache",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.[jt]s": [
      "npm run lint:eslint:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smelukov/revelation.git"
  },
  "author": "Sergey Melyukov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smelukov/revelation/issues"
  },
  "homepage": "https://github.com/smelukov/revelation#readme",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "commitlint": "^17.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "ts-jest": "^29.1.0",
    "memfs": "^3.5.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  }
}
